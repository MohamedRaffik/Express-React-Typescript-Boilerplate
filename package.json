{
  "name": "react-express-typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run build:server && npm run build:client",
    "lint": "eslint --fix src/**/*.ts && eslint --fix src/**/*.tsx",
    "test": "jest",
    "dev:server": "nodemon src/index.ts",
    "build:server": "tsc --build ./tsconfig.json",
    "test:server": "jest --clearCache && jest --rootdir=src/tests",
    "coverage:server": "npm run test:server -- --coverage",
    "dev:client": "webpack-dev-server",
    "build:client": "webpack --mode=production",
    "test:client": "jest --clearCache && jest --rootdir=src/client/src/tests",
    "coverage:client": "npm run test:client -- --coverage --coverageDirectory=src/client/coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0"
  }
}
